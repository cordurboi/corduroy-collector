PORT=4000

# Address of the deployed ERC-1155 NFT contract on Moonbase Alpha
NFT_CONTRACT_ADDRESS=YOUR_NFT_CONTRACT_ADDRESS

# Web3Auth client ID (from Web3Auth dashboard)
WEB3AUTH_CLIENT_ID=YOUR_WEB3AUTH_CLIENT_ID

# Pinata JWT for uploading metadata to IPFS
PINATA_JWT=YOUR_PINATA_JWT

# IPFS gateway used by the API to resolve metadata
PINATA_GATEWAY_URL=https://gateway.pinata.cloud/ipfs

# Private key for the relayer wallet that mints editions
RELAYER_PRIVATE_KEY=YOUR_RELAYER_PRIVATE_KEY

# Moonbase RPC and chain configuration
MOONBASE_RPC_URL=https://rpc.api.moonbase.moonbeam.network
CHAIN_ID=1287

# Simple admin token used to protect /admin endpoints
ADMIN_TOKEN=YOUR_ADMIN_TOKEN

# Map of human-friendly labels to edition IDs
EDITION_LABEL_MAP={"corduroy-demo-001":"1","corduroy-demo-002":"2","corduroy-demo-003":"3"}

